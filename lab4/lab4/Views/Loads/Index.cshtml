@{

    Layout = "/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<Lab3.Models.Load>
<!DOCTYPE html>
<html>
<head>
    <title>"Loads"</title>
</head>
<body>
    <table cellpadding="5">
        <tr>
            <th>LoadID</th>
            <th>Name</th>
            <th>Volume</th>
            <th>Weight</th>
        </tr>
        @foreach (var load in Model)
        {
            <tr>
                <td>@load.LoadId</td>
                <td>@load.LoadName</td>
                <td>@load.Volume</td>
                <td>@load.Weight</td>
            </tr>
        }
    </table>
</body>
</html>